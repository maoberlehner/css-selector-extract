{
  "name": "css-selector-extract",
  "description": "Extract certain CSS selectors form CSS code",
  "keywords": [
    "CSS"
  ],
  "version": "3.0.1",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/css-selector-extract",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maoberlehner/css-selector-extract"
  },
  "bugs": {
    "url": "https://github.com/maoberlehner/css-selector-extract/issues"
  },
  "dependencies": {
    "postcss": "^6.0.1"
  },
  "devDependencies": {
    "@avalanche/eslint-config": "^0.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "mocha": "^3.4.2",
    "postcss-scss": "^1.0.0",
    "pre-git": "^3.14.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1"
  },
  "main": "dist/index.js",
  "module": "js/index.js",
  "scripts": {
    "bundle": "rollup --config --format cjs --output dist/index.js js/index.js & rollup --config --format cjs --output dist/lib/postcss-selector-extract.js js/lib/postcss-selector-extract.js & rollup --config --format cjs --output dist/lib/filter-selector.js js/lib/filter-selector.js",
    "lint:no-error": "eslint js/* test/* || exit 0",
    "lint": "eslint js/* test/*",
    "build": "npm run bundle && npm run lint:no-error",
    "test:lib": "mocha test/lib/*.test.js --compilers js:babel-core/register",
    "test:index": "mocha test/index.test.js --compilers js:babel-core/register",
    "test": "mocha test/* --compilers js:babel-core/register"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm run lint"
      ],
      "pre-push": [
        "npm test"
      ]
    }
  }
}
