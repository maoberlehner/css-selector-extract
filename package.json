{
  "name": "css-selector-extract",
  "description": "Extract certain CSS selectors form (S)CSS code",
  "keywords": [
    "CSS"
  ],
  "version": "1.3.2",
  "author": "Markus Oberlehner",
  "homepage": "https://github.com/maoberlehner/css-selector-extract",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maoberlehner/css-selector-extract"
  },
  "bugs": {
    "url": "https://github.com/maoberlehner/css-selector-extract/issues"
  },
  "dependencies": {
    "postcss": "^5.2.6"
  },
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.4",
    "chai": "^3.5.0",
    "eslint": "^3.10.2",
    "eslint-config-modularis": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.1.2",
    "postcss-scss": "^0.4.0",
    "pre-git": "^3.11.1",
    "rollup": "^0.36.4",
    "rollup-plugin-buble": "^0.14.0"
  },
  "optionalDependencies": {
    "esdoc": "^0.4.8"
  },
  "main": "dist/index.js",
  "jsnext:main": "js/index.js",
  "scripts": {
    "scripts:bundle": "rollup --config --format cjs --output dist/index.js js/index.js & rollup --config --format cjs --output dist/lib/postcss-selector-extract.js js/lib/postcss-selector-extract.js & rollup --config --format cjs --output dist/lib/filter-selector.js js/lib/filter-selector.js",
    "scripts:lint": "eslint js/* test/*",
    "scripts:lint-no-error": "eslint js/* test/* || exit 0",
    "scripts:doc": "esdoc -c .esdocrc",
    "scripts": "npm run scripts:bundle && npm run scripts:lint-no-error",
    "test": "mocha"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm run scripts:lint"
      ],
      "pre-push": [
        "npm test"
      ]
    }
  }
}
